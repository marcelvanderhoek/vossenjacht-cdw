<% include header %>				
<!-- CONTENT START HIER -->

<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
	<h1 class="page-header">Realtime data</h1>
	<div class="table-responsive">
		<table class="table table-striped table-fixed" id="logboek">
			<thead>
				<tr>
					<th class="tijd">Tijd</th>
					<th>Bericht</th>
				</tr>
			</thead>
			<tbody class="logs">
				<% for(var i=0; i<logboek.length; i++) { %>
				    <tr>
				    	<% 
				    	var tijd = new Date(logboek[i].tijd); 
				    	function addZero(i) {
				    	    if (i < 10) {
				    	        i = "0" + i;
				    	    }
				    	    return i;
				    	}

			    	    var u = addZero(tijd.getHours());
			    	    var m = addZero(tijd.getMinutes());
			    	    var s = addZero(tijd.getSeconds());
				    	%>
				        <td class="tijdstip"><%=u+':'+m+':'+s %></td>
				    	
				    	<td><%=logboek[i].bericht %></td>
				    </tr>
				<% } %>
			</tbody>
		</table>
		
	</div>

	<h2 class="sub-header">Statistieken</h2>
